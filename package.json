{
  "name": "vue-ssr",
  "description": "Testing Vue.js SSR",
  "author": "Jared Johnson <jjohnson.eit@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "node server",
    "start": "cross-env NODE_ENV=production node server",
    "watch": "yarn build --watch",
    "build": "rimraf dist && yarn build:client && yarn build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.server.config.js --progress --hide-modules"
  },
  "dependencies": {
    "cross-env": "^5.2.0",
    "es6-promise": "^4.2.6",
    "express": "^4.16.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "handlebars": "^4.1.2",
    "jsdom": "^15.0.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.6",
    "vue-server-renderer": "^2.6.10",
    "vuex": "^3.1.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.12.0",
    "file-loader": "^3.0.1",
    "rimraf": "^2.6.3",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "3.8.1",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}
